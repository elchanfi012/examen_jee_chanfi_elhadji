<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>LogoutServlet</display-name>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>DashboardServlet</display-name>
		<servlet-name>DashboardServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.DashboardServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UserCardServlet</display-name>
		<servlet-name>UserCardServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.UserCardServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UsersListAdminServlet</display-name>
		<servlet-name>UsersListAdminServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.UsersListAdminServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UserAddAdminSErvlet</display-name>
		<servlet-name>UserAddAdminSErvlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.UserAddAdminSErvlet</servlet-class>
		<multipart-config>
	      <max-file-size>10485760</max-file-size>
	      <max-request-size>20971520</max-request-size>
	      <file-size-threshold>5242880</file-size-threshold>
	    </multipart-config>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UserEditAdminServlet</display-name>
		<servlet-name>UserEditAdminServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.UserEditAdminServlet</servlet-class>
		<multipart-config>
	      <max-file-size>10485760</max-file-size>
	      <max-request-size>20971520</max-request-size>
	      <file-size-threshold>5242880</file-size-threshold>
	    </multipart-config>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UserDeleteAdminServlet</display-name>
		<servlet-name>UserDeleteAdminServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.UserDeleteAdminServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UsersListServlet</display-name>
		<servlet-name>UsersListServlet</servlet-name>
		<servlet-class>examen_jee_chanfi_elhadji.servlets.UsersListServlet</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DashboardServlet</servlet-name>
		<url-pattern>/dashboard</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
	  	<url-pattern>*.css</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
	  	<url-pattern>*.js</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
	  	<url-pattern>*.jpg</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserCardServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UsersListAdminServlet</servlet-name>
		<url-pattern>/admin/users</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserAddAdminSErvlet</servlet-name>
		<url-pattern>/admin/user/add</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserEditAdminServlet</servlet-name>
		<url-pattern>/admin/user/edit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserDeleteAdminServlet</servlet-name>
		<url-pattern>/admin/user/delete</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UsersListServlet</servlet-name>
		<url-pattern>/users</url-pattern>
	</servlet-mapping>
	
	 <error-page>
	    <error-code>404</error-code>
	    <location>/404.jsp</location>
	  </error-page>
</web-app>